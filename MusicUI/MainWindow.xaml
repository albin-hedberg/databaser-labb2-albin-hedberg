<Window
    x:Class="MusicUI.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:MusicUI"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="clr-namespace:MusicUI.Views"
    Title="Labb 2"
    Width="1280"
    Height="720"
    FontSize="18"
    mc:Ignorable="d">
    <Grid>
        <TabControl>
            <TabItem Header="Start">
                <views:PlaylistView />
            </TabItem>
            <TabItem Header="Search">
                <views:SearchView />
            </TabItem>
            <TabItem Header="CRUD">
                <views:CRUDView />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
